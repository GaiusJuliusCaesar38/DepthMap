launch:
- node:
    pkg: "depth_projector"
    exec: "camera_pub"
    name: "camera_pub"
    namespace: "depth_projector"
    parameters:
        - fps: 30
        - frame_width: 1920
        - frame_height: 1080

- node:
    pkg: "depth_projector"
    exec: "depth_publisher"
    name: "depth_publisher"
    namespace: "depth_projector"
    parameters:
        - fps: 30
        - frame_width: 1920
        - frame_height: 1080

- node:
    pkg: "depth_projector"
    exec: "projector"
    name: "projector"
    namespace: "depth_projector"
